					<set_function>
						<type>radio</type>
						<values>
							<value>True</value>
							<value>False</value>
						</values>
					</set_function>
				</EXTENSION_GATEWAY2CHECKOUT_ENABLED>
			</configurations>
		</general>
	</tabs>
</ConfigurationGroup>
